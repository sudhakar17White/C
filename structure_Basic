//Basic of struct concept  

//how to intizaltion and how to delecration 
 
#include <stdio.h>

struct student //--->this is struct's tag naem student
{
    
    //delecration part only
    
    //we connot delecration inside the struct
    
    int roll;
    
    int age;
    
    float mark;
    
    
    
    
};

void main()
{ 
 
    struct student s1;//to create a object for struct in this line 
 
    s1.roll=1;
    
    s1.age=15;
    
    s1.mark=98.7;
    
    struct student s2;//to create a object for onther struct in this line 
 
    s2.roll=2;
    
    s2.age=16;
    
    s2.mark=90.7;
    
    
    
    // print all the value in the struct
    
    printf("First struct data's :\n");
    
    printf("%d\n",s1.roll);
    printf("%d\n",s1.age);
    printf("%f\n",s1.mark);
    
    
    printf("Second struct data's :\n");
    
    printf("%d\n",s2.roll);
    printf("%d\n",s2.age);
    printf("%f\n",s2.mark);
    
    
}






